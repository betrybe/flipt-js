{
  "name": "@betrybe/flipt-react",
  "version": "0.0.12",
  "description": "This library intends to be a Flipt client to React projects, enabling components rendering based on Feature Flags and supporting A/B testing.",
  "sideEffects": false,
  "cdn": "./dist/main.umd.js",
  "main": "./dist/main.js",
  "types": "./types/main.d.ts",
  "unpkg": "./dist/main.umd.js",
  "module": "./dist/main.esm.js",
  "jsdelivr": "./dist/main.umd.js",
  "umd:main": "./dist/main.umd.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/betrybe/flipt-js.git",
    "directory": "packages/flipt-react"
  },
  "exports": {
    ".": [
      {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "./dist/index.js"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/",
    "types/"
  ],
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint \"*/**/*.{ts,js,json}\"",
    "test:unit": "jest",
    "build": "rollup --config ./config/rollup.config.js"
  },
  "dependencies": {
    "@betrybe/flipt-sdk": "workspace:^0.0.6",
    "lodash": "^4.17.21"
  },
  "keywords": ["flipt", "react", "feature-flag", "fetch", "typescript", "typescript-types", "type", "types"],
  "author": "",
  "homepage": "https://github.com/betrybe/flipt-js/tree/main/packages/flipt-react#readme",
  "license": "Apache-2.0",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.21",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.65.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1"
  }
}
