{
  "name": "@betrybe/flipt-sdk",
  "version": "0.0.7",
  "description": "This library intends to be a Flipt client to JavaScript projects, enabling components rendering based on Feature Flags and supporting A/B testing.",
  "sideEffects": false,
  "cdn": "./dist/main.umd.js",
  "main": "./dist/main.js",
  "types": "./types/main.d.ts",
  "unpkg": "./dist/main.umd.js",
  "module": "./dist/main.esm.js",
  "jsdelivr": "./dist/main.umd.js",
  "umd:main": "./dist/main.umd.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/betrybe/flipt-js.git",
    "directory": "packages/flipt-react"
  },
  "exports": {
    ".": [
      {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "./dist/index.js"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/",
    "types/"
  ],
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint \"*/**/*.{ts,js,json}\"",
    "test:unit": "jest",
    "lint": "eslint \"*/**/*.{ts,js,json}\" --fix",
    "build": "rollup --config ./config/rollup.config.js"
  },
  "keywords": [
    "flipt",
    "sdk",
    "feature-flag",
    "fetch",
    "typescript",
    "typescript-types",
    "type",
    "types"
  ],
  "author": "",
  "license": "Apache-2.0",
  "homepage": "https://github.com/betrybe/flipt-js/tree/main/packages/flipt-sdk#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.25",
    "jest": "^27.5.1",
    "rollup": "^2.68.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.1.3",
    "ts-jest-resolver": "^1.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}
